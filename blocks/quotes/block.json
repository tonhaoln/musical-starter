{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ehd/quotes",
	"title": "Quotes",
	"description": "A carousel of rotating quotes",
	"style": "file:../../dist/css/quotes/quotes.css",
	"editorStyle": "file:../../dist/css/quotes/editor.css",
	"editorScript": [],
	"script": "ehd-quotes-js",
	"category": "common",
	"icon": "format-quote",
	"keywords": [
		"",
		"",
		""
	],
	"acf": {
		"mode": "preview",
		"renderTemplate": "quotes.php"
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"alignWide": true,
		"alignContent": false,
		"alignText": false,
		"anchor": true,
		"className": true,
		"color": {
			"background": true,
			"gradients": false,
			"link": false,
			"text": false,
			"enableContrastChecker": false
		},
		"customClassName": true,
		"defaultStylePicker": true,
		"html": false,
		"multiple": true,
		"reusable": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": false,
			"lineHeight": false
		}
	},
	"attributes": {
		"variable": {
			"type": "string",
			"default": ""
		}
	},
	"example": {}
}