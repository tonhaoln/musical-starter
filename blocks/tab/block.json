{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "title": "Tab",
  "name": "ehd/tab",
  "version": "0.1.0",
  "category": "custom-blocks",
  "icon": "table-row-after",
  "parent": ["ehd/tabs", "ehd/native-accordion"],
  "description": "An inner panel for a tab or accordion block.",
  "supports": {
    "html": false,
    "anchor": true,
    "align": true
  },
  "attributes": {
    "className": {
      "type": "string",
      "default": ""
    },
    "tabLabel": {
      "type": "string",
      "default": ""
    },
    "parentBlock": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "oots-plugin",
  "editorScript": "file:../../dist/js/tab-index.js",
  "editorStyle": "file:../../dist/css/tab/editor.css",
  "render": "file:./index.php"
}
