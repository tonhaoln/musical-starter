{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ehd/video",
	"title": "Video",
	"description": "A video block for YouTube or Vimeo",
	"style": "file:../../dist/css/video/video.css",
	"editorStyle": "file:../../dist/css/video/editor.css",
	"script": "ehd-video-js",
	"category": "common",
	"icon": "video-alt3",
	"keywords": ["youtube", "vimeo", "embed"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "render.php"
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"alignWide": true,
		"alignContent": false,
		"alignText": false,
		"anchor": true,
		"className": true,
		"color": {
			"background": true,
			"gradients": false,
			"link": false,
			"text": false,
			"enableContrastChecker": false
		},
		"customClassName": true,
		"defaultStylePicker": true,
		"html": false,
		"multiple": true,
		"reusable": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": false,
			"lineHeight": false
		}
	},
	"attributes": {
		"variable": {
			"type": "string",
			"default": ""
		}
	},
	"example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "source": "youtube",
        "videoId": "dQw4w9WgXcQ"
      }
    }
  }
}