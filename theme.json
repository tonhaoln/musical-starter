{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "layout": {
            "contentSize": "800px",
            "wideSize": "1000px"
        },
        "custom": {
            "lineHeight": {
                "heading": 1.2
            },
            "fontWeight": {
                "light": 300,
                "normal": 400,
                "semiBold": 600,
                "bold": 700
            }
        },
        "color": {
            "defaultPalette": false,
            "defaultGradients": false,
            "palette": [
                {
                    "name": "Dark Blue",
                    "slug": "dark-blue",
                    "color": "#25274D"
                },
                {
                    "name": "Grey",
                    "slug": "grey",
                    "color": "#aaabb8"
                },
                {
                    "name": "Navy Blue",
                    "slug": "navy-blue",
                    "color": "#464866"
                },
                {
                    "name": "Bright Blue",
                    "slug": "bright-blue",
                    "color": "#2e9cca"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#fff"
                }
            ]
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": true,
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "size": "clamp(0.75rem, 0.5761rem + 0.8696vi, 1.25rem)",
                    "slug": "10",
                    "name": "Half Rhythm"
                },
                {
                    "size": "clamp(1rem, 0.75rem + 1vi, 1.5rem)",
                    "slug": "20",
                    "name": "Rhythm"
                },
                {
                    "size": "clamp(1.5rem, 0.75rem + 3vi, 3rem)",
                    "slug": "40",
                    "name": "Small"
                },
                {
                    "size": "clamp(3rem, 1.5rem + 6vi, 6rem)",
                    "slug": "60",
                    "name": "Large"
                },
                {
                    "size": "clamp(4rem, 2rem + 8vi, 8rem)",
                    "slug": "80",
                    "name": "Huge"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
                    "slug": "default",
                    "name": "Default"
                }
            ],
            "fluid": true,
            "fontWeight": true,
            "lineHeight": true,
            "customFontSize": true,
            "defaultFontSizes": false,
            "fontSizes": [
                {
                    "slug": "tiny",
                    "size": "0.675em",
                    "name": "Tiny"
                },
                {
                    "slug": "small",
                    "size": "0.875em",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "name": "Normal",
                    "size": "1rem",
                    "fluid": {
                        "min": "1.2rem",
                        "max": "1rem"
                    }
                },
                {
                    "slug": "normal2",
                    "name": "Normal +",
                    "size": "1.075rem",
                    "fluid": {
                        "min": "1rem",
                        "max": "1.15rem"
                    }
                },
                {
                    "slug": "medium",
                    "name": "Medium",
                    "size": "1.15rem",
                    "fluid": {
                        "min": "1rem",
                        "max": "1.25rem"
                    }
                },
                {
                    "slug": "large",
                    "name": "Large",
                    "size": "1.25rem",
                    "fluid": {
                        "min": "1.1rem",
                        "max": "1.5rem"
                    }
                },
                {
                    "slug": "x-large",
                    "name": "Extra Large",
                    "size": "1.5rem",
                    "fluid": {
                        "min": "1.25rem",
                        "max": "1.75rem"
                    }
                },
                {
                    "slug": "huge",
                    "name": "Huge",
                    "size": "2.3rem",
                    "fluid": {
                        "min": "2rem",
                        "max": "2.7rem"
                    }
                },
                {
                    "slug": "gigantic",
                    "name": "Gigantic",
                    "size": "2rem",
                    "fluid": {
                        "min": "2.5rem",
                        "max": "3.5rem"
                    }
                }
            ]
        },
        "blocks": {}
    },
    "styles": {
        "typography": {
            "lineHeight": "",
            "fontFamily": "var(--wp--preset--font-family--default)"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--gigantic)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal2)",
                    "lineHeight": "var(--wp--custom--line-height--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--heading)"
                }
            }
        }
    }
}